<hgroup>
    <h1>Users</h1>
    <p><mark>Notice</mark> This page is a WIP.</p>
</hgroup>
<table>
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
            <th scope="col">Created</th>
        </tr>
    </thead>
    <tbody>
        {% for user in users %}
        <tr>
            <th scope="row">{{ user.id }}</th>
            <td>{{ user.username }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.created_on }}</td>
        </tr>
        {% endfor %}
</table>
